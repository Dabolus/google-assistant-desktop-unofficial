{
  "name": "google-assistant-desktop-unofficial",
  "productName": "Google Assistant",
  "version": "0.2.0",
  "description": "A desktop version for the Google Assistant (UNOFFICIAL)",
  "main": "main.js",
  "author": "Giorgio Garasto <giorgio@garasto.it>",
  "license": "MIT",
  "repository": "Dabolus/google-assistant-desktop-unofficial",
  "homepage": "https://github.com/Dabolus/google-assistant-desktop-unofficial",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=64005 ELECTRON_DISABLE_SECURITY_WARNINGS=1 run-p start:electron start:main start:renderer",
    "start:electron": "ts-node scripts/dev",
    "start:main": "webpack -w --config webpack/main.dev.config.ts",
    "start:renderer": "webpack-dev-server --config webpack/renderer.dev.config.ts --color",
    "build": "run-p build:main build:renderer && yarn build:electron",
    "build:electron": "ts-node scripts/prepare-build && cd app && electron-builder -c ../electron-builder.json",
    "build:main": "webpack --config webpack/main.prod.config.ts",
    "build:renderer": "webpack --config webpack/renderer.prod.config.ts",
    "upload": "ts-node scripts/upload",
    "lint": "eslint --ext .ts --ext .js .",
    "format": "eslint --ext .ts --ext .js . --fix",
    "postinstall": "electron-builder install-app-deps",
    "test": "cross-env NODE_ENV=test jest -c jest/base.config.js",
    "test:main": "cross-env NODE_ENV=test jest -c jest/main.config.js",
    "test:renderer": "cross-env NODE_ENV=test jest -c jest/renderer.config.js"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.4.5",
    "core-js": "^3.1.4",
    "electron-store": "^3.3.0",
    "electron-updater": "^4.0.6",
    "google-auth-library": "^4.2.1",
    "node-fetch": "^2.6.0",
    "nodejs-assistant": "^0.7.1",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@material/mwc-ripple": "^0.6.0",
    "@piuccio/ejs-compiled-loader": "^3.0.0",
    "@skatejs/ssr": "^0.19.11",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/form-data": "^2.2.1",
    "@types/fs-extra": "^7.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.15",
    "@types/mini-css-extract-plugin": "^0.2.1",
    "@types/node": "^12.0.8",
    "@types/node-fetch": "^2.3.7",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/wait-on": "^3.2.0",
    "@types/webpack": "^4.4.32",
    "@types/webpack-dev-server": "^3.1.6",
    "@types/webpack-env": "^1.13.9",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "autoprefixer": "^9.6.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-template-html-minifier": "^3.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "ejs": "^2.6.2",
    "electron": "^5.0.4",
    "electron-builder": "^20.43.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.1.1",
    "electron-watch": "^1.0.9",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "form-data": "^2.3.3",
    "fs-extra": "^8.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "^24.8.0",
    "lit-element": "^2.2.0",
    "lit-html": "^1.1.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.3",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "terser-webpack-plugin": "^1.3.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "browserslist": [
    "chrome >= 73"
  ]
}
