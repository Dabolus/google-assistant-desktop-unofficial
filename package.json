    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "browser-sync": "^2.24.4",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "electron": "^2.0.1",
    "electron-builder": "^20.13.4",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "grpc": "^1.11.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
