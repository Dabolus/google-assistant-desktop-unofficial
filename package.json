{
  "name": "google-assistant-desktop",
  "productName": "Google Assistant Desktop",
  "version": "0.0.1",
  "description": "Google Assistant desktop client",
  "main": "main.js",
  "author": {
    "name": "Giorgio Garasto (Dabolus)",
    "email": "mail@dabolus.io",
    "url": "https://dabolus.io"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "refresh-gens": "bash generate_assistant_gens.sh",
    "postinstall": "electron-builder install-app-deps",
    "serve:webpack": "webpack --watch --config ./webpack/dev.config.js",
    "serve:electron": "wait-on http-get://localhost:9000/ && electron dev",
    "serve": "run-p serve:webpack serve:electron",
    "build": "webpack --config ./webpack/prod.config.js",
    "build:win": "yarn run build && electron-builder build --windows",
    "serve:dist": "electron dist"
  },
  "dependencies": {
    "@dabolus/localized-lit-element": "^0.2.1",
    "@polymer/app-layout": "^3.0.0-pre.19",
    "@polymer/lit-element": "^0.5.1",
