{
  "name": "google-assistant-desktop-unofficial",
  "version": "1.0.0",
  "description": "A desktop version for the Google Assistant (UNOFFICIAL)",
  "main": "main.js",
  "author": "Giorgio Garasto <giorgio@garasto.it>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=1 run-p start:electron start:main start:renderer",
    "start:electron": "wait-on http://localhost:8080 && cp package.json node_modules/.build && electron node_modules/.build",
    "start:main": "webpack -w --config webpack/main.dev.config.ts",
    "start:renderer": "webpack-dev-server --config webpack/renderer.dev.config.ts --hot --color --compress --overlay",
    "build": "run-p build:main build:renderer && yarn build:electron",
    "build:electron": "cp package.json app && cd app && electron-builder --config ../electron-builder.json",
    "build:main": "webpack --config webpack/main.prod.config.ts",
    "build:renderer": "webpack --config webpack/renderer.prod.config.ts",
    "lint": "tslint -p .",
    "format": "tslint -p . --fix",
    "postinstall": "electron-builder install-app-deps",
    "test": "cross-env NODE_ENV=test jest -c jest/base.config.js",
    "test:main": "cross-env NODE_ENV=test jest -c jest/main.config.js",
    "test:renderer": "cross-env NODE_ENV=test jest -c jest/renderer.config.js"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@material/mwc-ripple": "^0.4.0",
    "@piuccio/ejs-compiled-loader": "^3.0.0",
    "@skatejs/ssr": "^0.19.11",
    "@types/copy-webpack-plugin": "^4.4.3",
    "@types/electron-store": "^1.3.1",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.11",
    "@types/loader-utils": "^1.1.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.11.6",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/redux-logger": "^3.0.7",
    "@types/webpack": "^4.4.26",
    "@types/webpack-env": "^1.13.9",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-node-externals": "^1.6.3",
    "autoprefixer": "^9.5.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-template-html-minifier": "^2.2.1",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "ejs": "^2.6.1",
    "electron": "^4.1.1",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-store": "^3.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "^24.5.0",
    "lit-element": "^2.1.0",
    "lit-html": "^1.0.0",
    "loader-utils": "^1.2.3",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.0",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "terser-webpack-plugin": "^1.2.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000",
    "wait-on": "^3.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "browserslist": [
    "chrome >= 66"
  ]
}
