{
  "name": "google-assistant-desktop-unofficial",
  "productName": "Google Assistant",
  "version": "0.2.0",
  "description": "A desktop version for the Google Assistant (UNOFFICIAL)",
  "main": "main.js",
  "author": "Giorgio Garasto <giorgio@garasto.it>",
  "license": "MIT",
  "repository": "Dabolus/google-assistant-desktop-unofficial",
  "homepage": "https://github.com/Dabolus/google-assistant-desktop-unofficial",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=64005 ELECTRON_DISABLE_SECURITY_WARNINGS=1 run-p start:electron start:main start:renderer",
    "start:electron": "ts-node scripts/dev",
    "start:main": "webpack -w --config webpack/main.dev.config.ts",
    "start:renderer": "webpack-dev-server --config webpack/renderer.dev.config.ts --color",
    "build": "run-p build:main build:renderer && yarn build:electron",
    "build:electron": "ts-node scripts/prepare-build && cd app && electron-builder -c ../electron-builder.json",
    "build:main": "webpack --config webpack/main.prod.config.ts",
    "build:renderer": "webpack --config webpack/renderer.prod.config.ts",
    "lint": "eslint --ext .ts --ext .js .",
    "fix": "eslint --ext .ts --ext .js . --fix",
    "format": "prettier --write '**/*.{js,ts,scss}'",
    "postinstall": "electron-builder install-app-deps",
    "test": "cross-env NODE_ENV=test jest -c jest/base.config.js",
    "test:main": "cross-env NODE_ENV=test jest -c jest/main.config.js",
    "test:renderer": "cross-env NODE_ENV=test jest -c jest/renderer.config.js",
    "add-locale": "lingui add-locale",
    "extract-locales": "lingui extract"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.7.2",
    "core-js": "2",
    "electron-store": "^5.1.0",
    "electron-updater": "^4.2.0",
    "google-auth-library": "^5.5.1",
    "node-fetch": "^2.6.0",
    "nodejs-assistant": "^0.7.1",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@lingui/cli": "^2.8.3",
    "@lingui/core": "^2.8.3",
    "@lingui/loader": "^2.8.3",
    "@lingui/macro": "^2.8.3",
    "@material/mwc-ripple": "^0.10.0",
    "@piuccio/ejs-compiled-loader": "^3.0.0",
    "@skatejs/ssr": "^0.19.11",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.22",
    "@types/lingui__core": "^2.7.0",
    "@types/lingui__macro": "^2.7.3",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/node": "^12.12.6",
    "@types/node-fetch": "^2.5.3",
    "@types/wait-on": "^3.2.0",
    "@types/webpack": "^4.39.8",
    "@types/webpack-dev-server": "^3.4.0",
    "@types/webpack-env": "^1.14.1",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "autoprefixer": "^9.7.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-template-html-minifier": "^3.1.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "ejs": "^2.7.1",
    "electron": "^7.1.0",
    "electron-builder": "^22.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.2.0",
    "electron-watch": "^1.0.9",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "form-data": "^3.0.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jest": "^24.9.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "remark": "^11.0.2",
    "remark-html": "^10.0.0",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "terser-webpack-plugin": "^2.2.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "browserslist": [
    "chrome >= 78"
  ]
}
